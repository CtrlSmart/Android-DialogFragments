<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="at.wada811.dialog.sample"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <!-- AlertDialogFragment Examples -->
        <activity
            android:name=".dialogfragmentcallbackprovider.DialogFragmentCallbackProviderFragmentActivity"
            android:label="@string/title_activity_dialog_fragment_callback_provider">
        </activity>
        <activity
            android:name=".alertdialogfragment.AlertDialogFragmentExamplesActivity"
            android:label="@string/title_activity_alert_dialog_fragment_examples_in_activity">
        </activity>
        <!-- ProgressDialogFragment Examples -->
        <activity
            android:name=".progressdialogfragment.ProgressDialogFragmentExamplesActivity"
            android:label="@string/title_activity_progress_dialog_fragment_examples">
        </activity>
        <activity
            android:name=".progressdialogfragment.examples.LoaderSpinnerProgressDialogFragmentExamplesActivity"
            android:label="@string/title_activity_progress_dialog_fragment_examples">
        </activity>
        <activity
            android:name=".progressdialogfragment.examples.LoaderSpinnerProgressDialogFragmentExamplesFragmentActivity"
            android:label="@string/title_activity_progress_dialog_fragment_examples">
        </activity>
        <activity
            android:name=".DatePickerDialogFragmentExamplesActivity"
            android:label="@string/title_activity_date_picker_dialog_fragment_examples">
        </activity>
        <activity
            android:name=".TimePickerDialogFragmentExamplesActivity"
            android:label="@string/title_activity_time_picker_dialog_fragment_examples">
        </activity>
        <activity
            android:name=".NumberPickerDialogFragmentExamplesActivity"
            android:label="@string/title_activity_number_picker_dialog_fragment_examples">
        </activity>
        <activity
            android:name=".StringPickerDialogFragmentExamplesActivity"
            android:label="@string/title_activity_string_picker_dialog_fragment_examples">
        </activity>

    </application>

</manifest>
